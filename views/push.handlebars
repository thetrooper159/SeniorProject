<h1 style="text-align: center">Send Push Notification</h1>
<br>
<div class="container" style="text-align: center">
 <form action="/ques" method="POST">
   <h3>Notification Title</h3>
   <input type="text" name="event" value=""><br><br>
   <h3>Date of Event</h3>
   <input type="date" name="date" value=""><br><br>
   <h3>House</h3>
   <input type="checkbox" name="neville" value="neville">
   <label for="neville">Neville</label><br>
   <input type="checkbox" name="shadyside" value="shadyside">
   <label for="shadyside">Shadyside</label><br>
   <input type="checkbox" name="universityplace" value="universityplace">
   <label for="universityplace">University Place</label><br>
   <h3>Event Location</h3>
   <textarea id="location" name="location" style="width: 400px; height: 50px" rows="10" cols="10" placeholder="Enter location"></textarea>
   <h3>Notification Description</h3>
   <textarea id="description" name="description" style="width: 400px; height: 170px" rows="10" cols="10" placeholder="Enter notification description"></textarea><br><br>
   <input type="submit">
 </form>
 console.log(container)
</div>
